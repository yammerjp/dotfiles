#!/usr/bin/env node

process.stdin.setEncoding("utf8");

const lines = [];

const reader = require("readline").createInterface({
  input: process.stdin,
});

reader.on("line", (line) => {
  lines.push(line);
});

reader.on("close", () => {
  let input = JSON.parse(lines.join("\n"));
  let output = eval("input " + process.argv.slice(2).join(' '));
  console.log(JSON.stringify(output, null, ' '));
});
